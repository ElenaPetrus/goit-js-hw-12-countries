
  {{#each this}}
  <h1 class="country-title">{{name}}</h1>
  <div class ="country-data">
  <div class="country-features">
    <p class="country-text"><b><span class="span-text">Capital:</span> {{capital}}</b></p>
    <p class="country-text"><b><span class="span-text">Population:</span> {{population}}</b></p>
    <p class="country-text"><b><span class="span-text">Languages:</span></b></p>
    <ul class="list-group"></ul>
    {{#each languages}}
      <li class="list-group-item">{{this.name}}</li>
    {{/each}}
    </ul>
  </div>

 <div class="country-flag-img">
    <img src="{{flag}}" alt="Flag of {{name}}">
  </div>
  </div>
 {{/each}}

